{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"  // Menentukan backend berbasis Node.js
    },
    {
      "src": "index.html",
      "use": "@vercel/static" // Menentukan file statis (frontend)
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1" // Route untuk backend API
    },
    {
      "src": "/(.*)",
      "dest": "/index.html" // Route untuk frontend
    }
  ]
}

